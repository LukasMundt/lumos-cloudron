{
  "id": "lumos.cloudronapp.php74",
  "title": "Lumos",
  "author": "Lukas Mundt",
  "version": "0.3.1",
  "healthCheckPath": "/health",
  "httpPort": 80,
  "manifestVersion": 2,
  "icon": "logo.png",
  "addons": {
    "mysql": {},
    "localstorage": {},
    "sendmail": {},
    "recvmail": {},
    "redis": {
      "noPassword": true
    },
    "scheduler": {
      "run_scheduler": {
        "schedule": "* * * * *",
        "command": "cd /app/code && php artisan schedule:run"
      }
    }
  },
  "tags": [
    "apache",
    "php",
    "mysql",
    "hosting",
    "lamp",
    "stacks",
    "development"
  ],
  "changelog": "file://CHANGELOG",
  "postInstallMessage": "file://POSTINSTALL.md",
  "multiDomain": false,
  "memoryLimit": 524288000,
  "minBoxVersion": "7.5.1"
}